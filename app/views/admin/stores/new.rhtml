<% render :partial => 'admin/shared/store_sub_menu' %>
<%= error_messages_for :store %>
<h1><%= t("creating_store") %></h1>
<% form_for :store,@store, :url => { :action => "create"}, :html => {:multipart => true} do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <p class="form-buttons">
    <%= button t("create") %> <%= link_to t('cancel'), collection_url %>
  </p>
<% end %>




